/* common/tpls/banner3/banner3.wxss */
.banner3 {
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  width: 100%;
  height: 300rpx;
  background-color: #eeeeee;
}

.banner3>view>navigator>image {
  width: 100%;
  height: 100%;
}

.banner3>view:nth-child(1) {
  margin-right: 10rpx;
  flex: 0 0 440rpx;
}

.banner3>view:nth-child(1)>navigator>image {
  width: 100%;
  height: 100%;
}

.banner3>view:nth-child(2) {
  flex: 0 0 300rpx;
  flex-direction: column;
  flex-wrap: wrap;
}

.banner3>view:nth-child(2)>navigator {
  width: 100%;
  height: 145rpx;
}

.banner3>view:nth-child(2)>navigator:nth-child(1) {
  margin-bottom: 10rpx;
}
